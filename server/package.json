{
  "name": "lolpro-online-server",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc && npm run copy-assets",
    "copy-assets": "node -e \"const fs=require('fs');const path=require('path');const dir='dist/database';if(!fs.existsSync(dir)){fs.mkdirSync(dir,{recursive:true});}fs.copyFileSync('src/database/schema.sql','dist/database/schema.sql');\"",
    "start": "node dist/index.js",
    "prestart": "npm run build"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@types/multer": "^2.0.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "google-auth-library": "^10.5.0",
    "jsonwebtoken": "^9.0.2",
    "mariadb": "^3.2.2",
    "multer": "^2.0.2",
    "node-cron": "^3.0.3",
    "socket.io": "^4.7.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.5",
    "@types/node-cron": "^3.0.11",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
